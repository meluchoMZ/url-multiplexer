{
	"manifest_version": 3,
	"name": "URL Multiplexer",
	"version": "1.0",
	"description": "Opens multiple tabs by appending suffixes to a base URI match.",
	"permissions": [
		"storage",
		"tabs",
		"declarativeNetRequest"
	],
	"host_permissions": [
		"<all_urls>"
	],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icons/logo-16.png",
			"32": "icons/logo-32.png",
			"34": "icons/logo-34.png",
			"48": "icons/logo-48.png",
			"128": "icons/logo-128.png"
		}
	},
	"background": {
		"scripts": ["background/background.js"]
	}
}
